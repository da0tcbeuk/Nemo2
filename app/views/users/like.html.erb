<%= stylesheet_link_tag 'like', :media => "all" %>

<h2>ユーザーがいいねした投稿一覧</h2>
<%= link_to "マイページ", user_path %>

<%= page_entries_info @deus %>
<% @user.liked_deus.each do |t| %>
  <h3>ユーザー名</h3>
  <%= t.user.name %>
  <h3>タイトル</h3>
  <%= t.Title %>
  <h3>本文</h3>
  <%= t.Text%>
  <h3>画像</h3>
  <%= image_tag t.image_url, size: "87x63" if t.image? %>
  
  <span><%= tag.name %></span>
<% end %>

<%= paginate @deus %>
